{
  "extends": ["@callstack",
    "plugin:prettier/recommended",
    "prettier"],
    "plugins": [
      "react",
      "prettier",
      "react-native", 
      "eslint-plugin-react-hooks"
  ],
  "rules": {
    "react-native/no-color-literals": 0,
    "react-native/no-inline-styles": 0,
    "react-native/no-raw-text": 0
  }
}